#include<stdio.h>
int main()
{
	int age;
	int hasID;
	printf("Enter your age");
	scanf("%d",&age);
	
	printf("Do you have a valid ID? (1 for Yes and 0 for no):");

	scanf("%d",&hasID);
	if(age>=18 && hasID==1){
		printf("You are eligible to vote\n");
		
	}
	else if(age>=18 && hasID==0) {
		printf("You meet the age requirement but can not vote");
		
	}
	else{
		printf("You are not eligible to vote\n");
		
	}
}
